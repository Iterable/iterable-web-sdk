{
  "name": "@iterable/web-sdk",
  "description": "Iterable SDK for JavaScript and Node.",
  "version": "0.0.1",
  "homepage": "https://iterable.com/",
  "repository": {
    "type": "git",
    "directory": "https://github.com/Iterable/iterable-web-sdk/example"
  },
  "engines": {
    "node": ">= 16.3.0"
  },
  "author": "Iterable",
  "license": "MIT",
  "private": false,
  "keywords": [
    "javscript",
    "iterable sdk",
    "typescript"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc && webpack",
    "start": "concurrently \"tsc -w --pretty\" \"webpack-dev-server\" -n 'tsc,webpack' -k",
    "test": "jest --config jest.config.js",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"src/**/*.js\"",
    "typecheck": "tsc --noEmit true --emitDeclarationOnly false",
    "lint": "eslint . --ext '.ts,.tsx'"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^24.0.25",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.2.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-webpack-plugin": "^2.4.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^27.0.6",
    "node-forge": "^0.10.0",
    "prettier": "^2.2.1",
    "selfsigned": "^1.10.8",
    "style-loader": "^1.0.1",
    "ts-jest": "^27.0.3",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {}
}
